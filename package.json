  "license": "MIT",
  "homepage": "https://github.com/michventura/starter#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-beta.8",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.15",
    "del-cli": "^4.0.1",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-custom-selectors": "^6.0.0",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.0.3",
    "postcss-preset-env": "^7.0.2",
    "prettier": "^2.5.1",
